{
  "shape": [96, 96, 96],
  "steps": 800,
  "rule": { "birth": [7, 8, 9], "survive": [4, 5] },
  "color_inheritance_mode": "hsv_boosted_mean",
  "color_params": {
    "saturation_boost": 1.4,
    "saturation_floor": 0.35
  },
  "mutation": {
    "enable": true,
    "per_birth_mutation_prob": 0.35,
    "per_step_mutation_prob": 0.0,
    "max_mutants_per_step": 1,
    "mutation_std": 60.0,
    "p_interval": 0.0
  },
  "seeds": [
    { "z": 48, "y": 48, "x": 42, "rgb": [255, 60, 60] },
    { "z": 48, "y": 42, "x": 48, "rgb": [60, 255, 60] },
    { "z": 42, "y": 48, "x": 48, "rgb": [60, 60, 255] },
    { "z": 52, "y": 48, "x": 48, "rgb": [255, 200, 40] },
    { "z": 48, "y": 52, "x": 48, "rgb": [40, 200, 255] },
    { "z": 48, "y": 48, "x": 52, "rgb": [200, 40, 255] }
  ],
  "outdir": "./out/starburst",
  "render_every": 1,
  "slice_every": 20,
  "render_slices": false,
  "create_gif": true,
  "gif_fps": 12,
  "delete_frames_after": true,
  "verbose": true,
  "random_state": 42
}