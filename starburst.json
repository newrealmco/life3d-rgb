{
  "shape": [96,96,96],
  "steps": 800,
  "rule": { "birth": [7,8,9], "survive": [4,5] },
  "color_mode": {
    "mode": "hsv_boosted_mean",
    "saturation_boost": 1.4,
    "saturation_floor": 0.35,
    "value_boost": 1.15
  },
  "mutation": { "per_birth_prob": 0.35, "std": 60, "per_step_prob": 0.0, "max_mutants_per_step": 1, "p_interval": 0.0 },
  "seeds": [
    { "z": 48, "y": 48, "x": 42, "rgb": [255, 60, 60] },
    { "z": 48, "y": 42, "x": 48, "rgb": [60, 255, 60] },
    { "z": 42, "y": 48, "x": 48, "rgb": [60, 60, 255] },
    { "z": 52, "y": 48, "x": 48, "rgb": [255, 200, 40] },
    { "z": 48, "y": 52, "x": 48, "rgb": [40, 200, 255] },
    { "z": 48, "y": 48, "x": 52, "rgb": [200, 40, 255] }
  ],
  "output": {
    "outdir": "out",
    "final_only": false,
    "high_res": { "enabled": false, "dpi": 300, "width_in": 12, "height_in": 12 },
    "gif": { "enabled": true, "fps": 12, "delete_frames_after": true }
  },
  "render": { "rotation_enabled": true, "deg_per_step": 2.5, "elevation": 30, "start_azim": 45 },
  "auto_stop": { "extinction": true, "steady": true, "steady_patience": 80 },
  "random_state": 42
}